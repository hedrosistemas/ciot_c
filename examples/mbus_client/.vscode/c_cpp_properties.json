{
    "env": {
        "IDF_COMPILER": "C:\\Users\\BINAR\\.espressif\\tools\\xtensa-esp-elf\\esp-14.2.0_20241119\\xtensa-esp-elf\\bin\\xtensa-esp32-elf-gcc.exe",
        "CIOT_C_PATH": "${CIOT_PATH}\\ciot_c",
        "HDR_DIR": "..//"
    },
    "configurations": [
        {
            "name": "windows-gcc-x64",
            "compilerPath": "C:/ProgramData/mingw64/mingw64/bin/gcc.exe ",
            "cStandard": "${default}",
            "cppStandard": "${default}",
            "intelliSenseMode": "windows-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerArgs": [
                "-DBOARD_WIN"
            ]
        },
        {
            "name": "ESP-IDF",
            "compilerPath": "${IDF_COMPILER}",
            "forcedInclude": [
                "${workspaceFolder}/build/esp32/config/sdkconfig.h"
            ],
            "includePath": [
                "${config:idf.espIdfPathWin}/components/**",
                "${CIOT_C_PATH}/include",
                "${CIOT_C_PATH}/src/proto/nanopb",
                "${CIOT_C_PATH}/libs/nanomodbus"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPathWin}/components/**",
                    "${CIOT_C_PATH}/src/esp32"

                ],
                "limitSymbolsToIncludedHeaders": false
            },
            "compilerArgs": [
                "-DESP_PLATFORM",
                "-DIDF_VER",
                "-DBOARD_HG3"
            ]
        }
    ],
    "version": 4
}